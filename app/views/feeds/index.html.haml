%h1 Dashboard Here

-if(@feed)
  -@feeds.each do |feed|
    %ul
      %ul
      %li= feed.title
      %li= feed.url
      - feed.entries.each do |entry|
        %ul
          %li= entry.title

-if(@hash_userfeeds)
  -@hash_userfeeds.each_pair do |category, arr_userfeeds|
    %ul
    %h3=category
    -arr_userfeeds.each do |userfeed|

      %li
        =link_to "#{userfeed.feed.title}", {controller: "feeds", action: "show", id: userfeed.feed.id}, id: userfeed.id
        = link_to image_tag('X_thumb.png'),{},class: "delete_feed", data: {userfeed_id: "#{userfeed.id}"}
      %ul
        / -userfeed.feed.entries.each do |entry|
        /   %li=entry.title
        /   %li=entry.summary







