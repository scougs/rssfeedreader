%h2 Your Feeds
%br
%br


#accordion
  -if(@hash_userfeeds)
    -@hash_userfeeds.each_pair do |category, arr_userfeeds|
      %h4
        =category
      %div.accordion-list
        %ul
          -arr_userfeeds.each do |userfeed|
            %li
              =link_to "#{userfeed.feed.title}", {controller: "feeds", action: "show", id: userfeed.feed.id}, id: userfeed.id


