%h1 Users
.user-feed-list
  -@users.each do |user|
    %h4
      =user.first_name
      =user.last_name
      %br
        %p.user-feed-list
          =link_to 'View User Profile', user
          -user.user_categories_and_feeds.each do |key, value|
            %ul.user-feed-list
              %li
                =key





